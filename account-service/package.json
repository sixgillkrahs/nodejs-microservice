{
  "name": "enterprise-service",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "prod": "node ./node_modules/moleculer/bin/moleculer-runner.js --repl --config dist/configs/moleculer.config.js --envfile env/application.properties dist/start.js",
    "build": "tsc",
    "dev": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config configs/moleculer.config.ts --envfile .run.dev.env start.ts",
    "uat": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config configs/moleculer.config.ts --envfile .run.uat.env start.ts",
    "local": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config configs/moleculer.config.ts --envfile .run.local.env start.ts",
    "release": "node ./node_modules/moleculer/bin/moleculer-runner.js --repl --config dist/configs/moleculer.config.js --envfile .run.env dist/start.js",
    "start": "npm run release"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "Goopay",
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "moleculer-repl": "^0.7.4",
    "ts-node": "^10.9.2",
    "xlsx-template": "^1.4.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "goopay-library": "^1.0.1",
    "ioredis": "^4.14.1",
    "lodash": "^4.17.21",
    "moleculer": "0.14.33",
    "moment": "^2.30.1",
    "mongoose": "^8.5.1",
    "mongoose-paginate-v2": "^1.4.1",
    "nats": "^2.22.0",
    "node-cache": "^5.1.2",
    "payment": "^2.4.6",
    "typescript": "^5.4.5",
    "request": "*",
    "express": "*",
    "jsonwebtoken": "*",
    "body-parser": "*",
    "fs": "*",
    "crypto": "*",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": ">= 20.x.x"
  },
  "main": ".eslintrc.js",
  "license": "ISC"
}